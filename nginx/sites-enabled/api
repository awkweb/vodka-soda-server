upstream api {
    server unix:/home/api/server.sock;
}

server {

    listen 9055;

    location /static/admin/ {
        types {
            text/css        css;
            text/javascript js;
            image/svg+xml	svg;
        }
        alias /home/api/static/admin/;
    }

    location /static/gis/ {
        types {
            text/css        css;
            text/javascript js;
            image/png	    png;
        }
        alias /home/api/static/gis/;
    }

    location /static/rest_framework/ {
        types {
            text/css        css;
            text/javascript js;
            image/png	    png;
        }
        alias /home/api/static/rest_framework/;
    }

    location /static/ {
        alias /home/api/static/;
    }

    location / {
		include		uwsgi_params;
		uwsgi_pass  api;
    }

}
